{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "BUTTONDOWN_API_KEY": "@buttondown-api-key"
  },
  "redirects": [
    {
      "source": "/launch",
      "destination": "/"
    },
    {
      "source": "learn-next-js",
      "destination": "https://leerob.io/blog/learn-nextjs"
    },
    {
      "source": "sitemap-robots",
      "destination": "https://leerob.io/blog/nextjs-sitemap-robots"
    },
    {
      "source": "nextjs-gatsby-create-react-app",
      "destination": "https://leerob.io/blog/nextjs-gatsby-create-react-app"
    }
  ]
}
